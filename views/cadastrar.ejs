<%- contentFor('secao1') %>

<h1> Cadastre-se </h1> <br>

<%- contentFor('secao2') %>

<div class="col-sm-8">

    <form action='/create' method='post'>

        <div class="form-group">
            <input type="text" size="40" name="user" id="user" class="form-control"  placeholder="Digite seu nome">
        </div>

        <div class="form-group">
            <input type="text" size="40" name="nick" id="nick" class="form-control"  placeholder="Digite seu nick">
        </div>

        <div class="form-group">
            <input type="password" size="40" name="pass" id="pass" class="form-control"  placeholder="Digite uma senha">
        </div>

        <br>
        <button type="submit" class="btn btn-primary">Cadastrar</button>

    </form>

</div>